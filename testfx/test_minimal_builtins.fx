// 最简化的新函数测试

println("abs(-5):")
println(abs(-5))

println("floor(3.7):")
println(floor(3.7))

println("ceil(3.2):")
println(ceil(3.2))

println("round(3.5):")
println(round(3.5))

println("sqrt(16):")
println(sqrt(16))

println("pow(2, 3):")
println(pow(2, 3))

println("min(3, 7):")
println(min(3, 7))

println("max(3, 7):")
println(max(3, 7))

println("random():")
r := random()
println(r)

println("startsWith('hello', 'he'):")
println(startsWith("hello", "he"))

println("endsWith('hello', 'lo'):")
println(endsWith("hello", "lo"))

println("contains('hello world', 'wo'):")
println(contains("hello world", "wo"))

println("All tests completed!")
