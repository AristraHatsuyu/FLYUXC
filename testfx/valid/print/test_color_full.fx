// 完整彩色输出测试

// 数字（应为黄色）
println(123)
println(3.14159)
println(-42)

// 字符串（无颜色）
println("Simple string")
println("It's a nice day")

// 布尔值（应为黄色）
println(true)
println(false)

// 空值（灰色加粗）
n :[num]= null
println(n)

// 数组（括号灰色，内容带颜色）
println([1, 2, 3])
println(["red", "green", "blue"])
println([true, false, true])
println([1, "mixed", true, [10, 20]])

// 对象（青色）
obj1 :[obj]= {x: 1, y: 2}
println(obj1)
